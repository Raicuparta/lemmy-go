{
  "name": "Lemmy Go",
  "description": "Quickly navigate to a Lemmy community from the address bar.",
  "version": "0.4.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "omnibox": {
    "keyword": "lc"
  },
  "host_permissions": [
    "https://lemmy.raicuparta.com/*"
  ],
  "permissions": [
    "storage"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "options.html"
  },
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "128": "icon.png"
  }
}
