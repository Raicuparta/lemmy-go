{
  "name": "Go To Lemmy Community",
  "description": "Quickly navigate to a Lemmy community from the address bar.",
  "version": "0.3.0",
  "manifest_version": 3,
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "lc"
  },
  "host_permissions": [
    "https://lemmy.raicuparta.com/*"
  ],
  "permissions": [
    "storage"
  ],
  "action": {
    "default_icon": {
      "16": "newtab_search16.png",
      "32": "newtab_search32.png",
      "48": "newtab_search48.png",
      "128": "newtab_search128.png"
    }
  },
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "16": "newtab_search16.png",
    "32": "newtab_search32.png",
    "48": "newtab_search48.png",
    "128": "newtab_search128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "lemmy-communities@raicuparta.com",
      "strict_min_version": "109.0"
    }
  }
}
